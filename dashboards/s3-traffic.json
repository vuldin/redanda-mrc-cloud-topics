{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "S3 Upload Throughput",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (instance) (rate(redpanda_cloud_storage_uploaded_bytes[5m]))",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "title": "S3 Download Throughput",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (instance) (rate(redpanda_cloud_storage_downloaded_bytes[5m]))",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 15,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "title": "S3 Upload Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le, instance) (rate(redpanda_cloud_storage_upload_latency_bucket[5m])))",
          "legendFormat": "p99 {{instance}}"
        },
        {
          "expr": "histogram_quantile(0.50, sum by (le, instance) (rate(redpanda_cloud_storage_upload_latency_bucket[5m])))",
          "legendFormat": "p50 {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "title": "S3 Download Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le, instance) (rate(redpanda_cloud_storage_download_latency_bucket[5m])))",
          "legendFormat": "p99 {{instance}}"
        },
        {
          "expr": "histogram_quantile(0.50, sum by (le, instance) (rate(redpanda_cloud_storage_download_latency_bucket[5m])))",
          "legendFormat": "p50 {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "title": "Envoy Upstream Request Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le, instance) (rate(envoy_cluster_upstream_rq_time_bucket{envoy_cluster_name=\"s3_mrap\"}[5m])))",
          "legendFormat": "p99 {{instance}}"
        },
        {
          "expr": "histogram_quantile(0.50, sum by (le, instance) (rate(envoy_cluster_upstream_rq_time_bucket{envoy_cluster_name=\"s3_mrap\"}[5m])))",
          "legendFormat": "p50 {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          }
        }
      }
    },
    {
      "title": "Envoy S3 Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (instance) (rate(envoy_cluster_upstream_rq_total{envoy_cluster_name=\"s3_mrap\"}[5m]))",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 30,
            "lineWidth": 1
          }
        }
      }
    },
    {
      "title": "S3 Errors",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (instance) (rate(redpanda_cloud_storage_errors_total[5m]))",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        }
      }
    },
    {
      "title": "Envoy Upstream Errors (5xx)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (instance) (rate(envoy_cluster_upstream_rq_xx{envoy_cluster_name=\"s3_mrap\",envoy_response_code_class=\"5\"}[5m]))",
          "legendFormat": "5xx {{instance}}"
        },
        {
          "expr": "sum by (instance) (rate(envoy_cluster_upstream_rq_xx{envoy_cluster_name=\"s3_mrap\",envoy_response_code_class=\"4\"}[5m]))",
          "legendFormat": "4xx {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2
          },
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.1 }
            ]
          }
        }
      }
    }
  ],
